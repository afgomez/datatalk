<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(visualization) -%>">
  <span class='title'>
    <%= visualization.legend %>
    <span class="preview">&nbsp;</span>
  </span>
  <span class='actions'>
    <%= link_to refinery_icon_tag("application_go.png"), visualization_url(visualization),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    <%= link_to refinery_icon_tag("application_edit.png"), edit_admin_visualization_path(visualization),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), admin_visualization_path(visualization),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'shared.admin.delete', :title => visualization.legend),
        :method => :delete %>
  </span>
</li>
