<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(analysis) -%>">
  <span class='title'>
    <%= analysis.title %>
    <span class="preview">&nbsp;</span>
  </span>
  <span class='actions'>
    <%= link_to refinery_icon_tag("application_go.png"), analysis_url(analysis),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    <%= link_to refinery_icon_tag("application_edit.png"), edit_admin_analysis_path(analysis),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), admin_analysis_path(analysis),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'shared.admin.delete', :title => analysis.title),
        :method => :delete %>
  </span>
</li>
