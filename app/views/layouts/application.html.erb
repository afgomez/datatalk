<!DOCTYPE html>
<html>
<head>
  <title>Datatalk</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults, 'highcharts' %>
  <%= csrf_meta_tag %>
</head>
<body<%= body_id %>>

  <div class="lista_cabecera">
    <div class="wrapper">
    <ul>
        <li><%= login_or_logout_path %> | </li>
        <li><%= create_or_edit_user_registration_path %> |</li>
  	<li><%= link_to 'Panel de control', dashboard_path %> |</li>
    </ul>
    </div>
  </div>

<div class="wrapper">


  <header class="c">
    <h1 class="cabecera">Datatalk</h1>
    <p class="tagline">Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut</p>
    <!-- <div class="header_tools">
    <form class ="buscador" action="form_action.asp" method="get">
    <input type="text" name="fname" />
    </form>
    </div> -->

  </header>

	<nav class=="cabecera_navegacion">
		<%= link_to 'Home', '/' %>
		<a href="analisis.html">Análisis</a>
		<%= link_to 'Datasets', datasets_path %>
		<a href="about.html">¿Qué es esto?</a>
		<a href="http://www.abredatos.es">www.abredatos.es</a>
		<a href="http://www.twitter.com/abredatos">Twitter de Abredatos</a>
	</nav>
  
  <% [:notice, :alert].each do |k| %>
    <% unless flash[k].blank? %>
      <div class="flash <%= k %>"><%= flash[k] %></div>
    <% end %>
  <% end %>
  
  <div id="container">
    <%= yield %>
  </div>

  <footer class="f">
  	<p>Datatalk - Abredatos</p>
  </footer>

</div><!-- wrapper -->

</body>
</html>
