<% form_for @analysis do |f| %>
  <%= render "shared/form_errors", :object => @analysis %>

  <%= render :partial => 'form_analysis_fields', :locals => {:f => f}%>
  <%= render :partial => 'form_visualization_fields', :locals => {:f => f}%>

  <%= f.hidden_field :analysis_id -%>

  <div class="form-actions">
    <%= f.submit "Responder" -%>
  </div>
<% end %>
